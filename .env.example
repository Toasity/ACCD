# Example environment variables (fill and copy to .env)
# Required: provide your CoinMetrics API key
COINMETRICS_API_KEY=

# Postgres connection (change if needed)
POSTGRES_DB=coinmetrics
POSTGRES_USER=coinmetrics
POSTGRES_PASSWORD=coinmetrics
POSTGRES_PORT=5432

# Optional: asset/metric selection and time range (defaults)
CM_ASSETS=btc,eth
CM_METRICS=PriceUSD,TxCnt
CM_START_DATE=2013-01-01
CM_END_DATE=2015-12-31
CM_FREQUENCY=1d
